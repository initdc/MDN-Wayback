{
  "manifest_version": 3,
  "name": "MDN Wayback",
  "description": "Browse the old style MDN Web Docs.",
  "version": "0.0.1",
  "icons": {
    "32": "/images/favicon-32x32.png",
    "48": "/images/favicon-48x48.png"
  },
  "homepage_url": "https://moz.one/",
  "permissions": ["storage", "activeTab", "scripting"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+M",
        "mac": "MacCtrl+M"
      }
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": ""
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content-script.js"],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start"
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
